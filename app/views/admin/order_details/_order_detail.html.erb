<% @order_details.each_with_index do |order_detail, index| %>
  <tr>
    <td><%= create_index params[:page], index, Settings.paginate.per_page %></td>
    <td class="th-product"><%= image_tag order_detail.product_picture.url %></td>
    <td><%= order_detail.product_name %></td>
    <td><%= order_detail.quantity %></td>
    <td><%= order_detail.product_price %></td>
    <td><%= calc_price order_detail.product_price, order_detail.quantity %></td>
  </tr>
<% end %>
